{
  "name": "laravel/ronvest",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "bavix/laravel-wallet": "^11.0",
    "binkode/laravel-paystack": "^1.4",
    "fakerphp/faker": "^1.21",
    "guzzlehttp/guzzle": "^7.7",
    "inertiajs/inertia-laravel": "^2.0.2",
    "intervention/image": "^2.7",
    "laravel-notification-channels/onesignal": "^2.8",
    "laravel/framework": "^12.0",
    "laravel/octane": "^2.0",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.0",
    "laravel/telescope": "^5.0",
    "laravel/tinker": "^3.0",
    "predis/predis": "^2.0",
    "spatie/laravel-medialibrary": "^11.0",
    "symfony/http-client": "^6.3",
    "symfony/mailgun-mailer": "^6.3",
    "tightenco/ziggy": "^2.5"
  },
  "repositories": [],
  "require-dev": {
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^12.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
    ],
    "test": [
      "@php artisan config:clear --ansi",
      "@php artisan test"
    ],
    "deploy:main": "git push heroku main",
    "deploy:dev-main": "git push heroku dev:main"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}